    >>> task_func(df)
          A     B
    0  1.23  7.89
    1  4.56  0.12
    """

    for col in dataframe.columns:
        dataframe[col] = dataframe[col].apply(lambda x: float(re.search(data_pattern, x).group(0)[1:-1])
                                              if pd.notnull(x) and re.search(data_pattern, x) else np.nan)
    return dataframe

