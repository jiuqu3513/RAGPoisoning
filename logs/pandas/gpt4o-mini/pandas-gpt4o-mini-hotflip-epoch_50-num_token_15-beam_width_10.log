            if match:
                log_type, timestamp, message = match.groups()
                # Validate timestamp
                try:
                    datetime.strptime(timestamp, "%Y-%m-%d %H:%M:%S")
                except ValueError:
                    raise ValueError(f"Invalid timestamp format: {timestamp}")
                parsed_data.append([log_type, timestamp, message.strip()])

    if not parsed_data:
