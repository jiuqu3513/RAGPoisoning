    >>> len(df)
    5
    >>> df.iloc[0]['Price']
    249.81604753894499
    >>> ax.title.get_text()
    'Stock Prices'
    """

    if seed is not None:
        np.random.seed(seed)
    date_range = pd.date_range(start=start_date, periods=periods, freq=freq)
