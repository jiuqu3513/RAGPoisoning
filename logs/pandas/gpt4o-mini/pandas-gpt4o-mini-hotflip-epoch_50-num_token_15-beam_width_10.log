    plt.scatter(df['Age'], df['Score'], label='Data')
    plt.plot(df['Age'].values, df['Age_up'].values, 'r', label='Fitted line')
    plt.xlabel('Age')
    plt.ylabel('Score')
    plt.title('Linear Regression')
    plt.legend()
    return plt, ax

